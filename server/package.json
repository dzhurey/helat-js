    "lint": "npx eslint"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "datalize": "^0.3.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "moment": "^2.29.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.2",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  }
